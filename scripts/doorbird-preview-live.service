[Unit]
Description=DoorBird G1 dual live preview call
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
User=config
Group=config
ExecStart=/home/config/pi_baresip_preview_dual_live.sh 30
TimeoutStartSec=120
TimeoutStopSec=10
KillMode=mixed
RemainAfterExit=no

[Install]
WantedBy=multi-user.target
